{
  "servers": [
    {
      "id": "hf_server",
      "name": "Hugging Face MCP Server",
      "source": {
        "type": "Http",
        "url": "https://hf.co/mcp",
        "timeout_secs": 30,
        "headers": {
          "User-Agent": "mistral-rs/0.6.0"
        }
      },
      "enabled": true,
      "tool_prefix": "hf",
      "resources": null,
      "bearer_token": "hf_xxx"
    },
    {
      "id": "filesystem",
      "name": "Local Filesystem MCP",
      "source": {
        "type": "Process",
        "command": "mcp-server-filesystem",
        "args": ["--root", "/tmp", "--readonly"],
        "work_dir": null,
        "env": {
          "MCP_LOG_LEVEL": "info",
          "MCP_TIMEOUT": "30"
        }
      },
      "enabled": false,
      "tool_prefix": "fs",
      "resources": ["file://**"],
      "bearer_token": null
    },
    {
      "id": "web_search",
      "name": "Web Search MCP Server",
      "source": {
        "type": "Http",
        "url": "https://api.search.example.com/mcp",
        "timeout_secs": 30,
        "headers": {
          "X-API-Version": "v1",
          "User-Agent": "mistral-rs/0.6.0"
        }
      },
      "enabled": false,
      "tool_prefix": "web",
      "resources": null,
      "bearer_token": "your-search-api-token"
    },
    {
      "id": "realtime_data",
      "name": "Real-time Data MCP",
      "source": {
        "type": "WebSocket",
        "url": "wss://data.example.com/mcp",
        "timeout_secs": 60,
        "headers": {
          "Origin": "https://mistral.rs",
          "Sec-WebSocket-Protocol": "mcp"
        }
      },
      "enabled": false,
      "tool_prefix": "rt",
      "resources": null,
      "bearer_token": "your-websocket-token"
    }
  ],
  "auto_register_tools": true,
  "tool_timeout_secs": 30,
  "max_concurrent_calls": 10
}